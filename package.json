{
  "name": "@dashevo/dpns-client",
  "version": "0.2.0-dev.3",
  "description": "Javascript client for DPNS",
  "main": "lib/DPNSClient.js",
  "unpkg": "dist/dpns-client.min.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "webpack --display-error-details",
    "check-package": "npm run check-package:name && npm run check-package:version",
    "check-package:name": "test $(jq -r .name package.json) = $(jq -r .name package-lock.json)",
    "check-package:version": "test $(jq -r .version package.json) = $(jq -r .version package-lock.json)",
    "lint": "eslint .",
    "test": "npm run test:unit && npm run test:functional && npm run test:browsers",
    "test:unit": "mocha 'test/unit/**/*.spec.js'",
    "test:functional": "mocha 'test/functional/**/*.spec.js'",
    "test:browsers": "karma start ./karma.conf.js --single-run",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashevo/js-dpns-client.git"
  },
  "keywords": [
    "dpns",
    "client"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashevo/js-dpns-client/issues"
  },
  "homepage": "https://github.com/dashevo/js-dpns-client#readme",
  "devDependencies": {
    "@dashevo/dp-services-ctl": "~0.11.0",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "dotenv-expand": "^5.1.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@dashevo/dapi-client": "~0.9.0-dev.7",
    "@dashevo/dashcore-lib": "~0.18.0",
    "@dashevo/dpns-contract": "~0.2.0-dev.4",
    "@dashevo/dpp": "~0.11.0",
    "bs58": "^4.0.1"
  }
}
