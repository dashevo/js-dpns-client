{
  "name": "@dashevo/dpns-client",
  "version": "0.2.0-dev.1",
  "description": "Javascript client for DPNS dApp",
  "main": "lib/DPNSClient.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "check-package": "npm run check-package:name && npm run check-package:version",
    "check-package:name": "test $(jq -r .name package.json) = $(jq -r .name package-lock.json)",
    "check-package:version": "test $(jq -r .version package.json) = $(jq -r .version package-lock.json)",
    "lint": "eslint .",
    "test": "npm run test:unit && npm run test:functional",
    "test:unit": "mocha 'test/unit/**/*.spec.js'",
    "test:functional": "mocha 'test/functional/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashevo/js-dpns-client.git"
  },
  "keywords": [
    "dpns",
    "client"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashevo/js-dpns-client/issues"
  },
  "homepage": "https://github.com/dashevo/js-dpns-client#readme",
  "devDependencies": {
    "@dashevo/dp-services-ctl": "^0.7.4",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "@dashevo/dapi-client": "^0.6.5",
    "@dashevo/dashcore-lib": "^0.17.9",
    "@dashevo/dpns-contract": "^0.1.0",
    "@dashevo/dpp": "^0.8.0-dev.3"
  }
}
